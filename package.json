{
  "name": "@sjofartstidningen/bryggan",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:react-pdf && npm run build:app",
    "build:app": "remix build",
    "build:react-pdf": "sh ./scripts/build-react-pdf-worker.sh",
    "dev": "remix dev",
    "predev": "npm run build:react-pdf",
    "start": "remix-serve build",
    "docker:build": "docker build -f Dockerfile . -t remix/run",
    "lint": "eslint app --ext .js,.ts,.tsx",
    "format": "prettier app --write",
    "routes": "npx remix routes",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@reach/dialog": "0.18.0",
    "@reach/menu-button": "0.18.0",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "debug": "4.3.4",
    "dropbox": "^10.34.0",
    "framer-motion": "9.0.7",
    "isbot": "3.6.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-intersection-observer": "9.4.2",
    "react-pdf": "6.2.2",
    "react-use-measure": "2.1.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/debug": "4.1.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-pdf": "6.2.0",
    "eslint": "8.34.0",
    "patch-package": "6.5.1",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16"
  }
}
