{
  "name": "@sjofartstidningen/bryggan",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && npm run build:react-pdf && npm run build:app",
    "build:app": "remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build:react-pdf": "sh ./scripts/build-react-pdf-worker.sh",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:app\"",
    "dev:app": "remix dev",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "predev": "npm run build:react-pdf",
    "start": "remix-serve build",
    "docker:build": "docker build -f Dockerfile . -t remix/run",
    "lint": "eslint app --ext .js,.ts,.tsx",
    "format": "prettier app --write",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@reach/dialog": "^0.18.0",
    "@reach/menu-button": "^0.18.0",
    "@remix-run/node": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-run/serve": "^1.7.4",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "framer-motion": "^7.6.2",
    "isbot": "^3.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-intersection-observer": "^9.4.0",
    "react-pdf": "^5.7.2",
    "react-use-measure": "^2.1.1",
    "remix-auth": "^3.3.0",
    "remix-auth-oauth2": "^1.3.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/debug": "^4.1.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-pdf": "^5.7.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "patch-package": "^6.5.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "18.12.0"
  }
}
